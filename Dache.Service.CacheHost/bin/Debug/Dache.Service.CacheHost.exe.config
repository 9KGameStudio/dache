<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <!-- Config Sections -->
  <configSections>
    <section name="cacheHostSettings" type="Dache.Service.CacheHost.Configuration.CacheHostConfigurationSection, Dache.Service.CacheHost" allowExeDefinition="MachineToApplication"/>
  </configSections>
  <!-- Cache Host Settings -->
  <cacheHostSettings address="localhost" port="33333" cacheMemoryLimitPercentage="85">
    <cacheManager address="localhost" port="33334" />
  </cacheHostSettings>
  <!-- Server Mode GC -->
  <runtime>
    <gcServer enabled="true"/>
  </runtime>
</configuration>